    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="kl_feed.activity_manager.class">KL\FeedBundle\Activity\ActivityManager</parameter>
    </parameters>

    <services>
        <service id="kl_feed.activity_manager" class="%kl_feed.activity_manager.class%">
